    <Param name="exportHTML" updates="None" val="on Sync" valType="str"/>
    <Param name="logging level" updates="None" val="exp" valType="code"/>
  </Settings>
  <Routines>
    <Routine name="trial">
      <PolygonComponent name="bL">
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="durationEstim" updates="None" val="" valType="code"/>
        <Param name="fillColor" updates="set every repeat" val="$[1,1,1]" valType="str"/>
        <Param name="fillColorSpace" updates="constant" val="rgb" valType="str"/>
        <Param name="interpolate" updates="constant" val="linear" valType="str"/>
        <Param name="lineColor" updates="constant" val="$[1,1,1]" valType="str"/>
        <Param name="lineColorSpace" updates="constant" val="rgb" valType="str"/>
        <Param name="lineWidth" updates="constant" val="1" valType="code"/>
        <Param name="nVertices" updates="constant" val="4" valType="int"/>
        <Param name="name" updates="None" val="bL" valType="code"/>
        <Param name="opacity" updates="constant" val="1" valType="code"/>
        <Param name="ori" updates="constant" val="0" valType="code"/>
        <Param name="pos" updates="constant" val="(-0.8,0)" valType="code"/>
        <Param name="saveStartStop" updates="None" val="True" valType="bool"/>
        <Param name="shape" updates="constant" val="rectangle" valType="str"/>
        <Param name="size" updates="constant" val="(0.1, 0.5)" valType="code"/>
        <Param name="startEstim" updates="None" val="" valType="code"/>
        <Param name="startType" updates="None" val="time (s)" valType="str"/>
        <Param name="startVal" updates="None" val="0.0" valType="code"/>
        <Param name="stopType" updates="None" val="condition" valType="str"/>
        <Param name="stopVal" updates="constant" val="" valType="code"/>
        <Param name="syncScreenRefresh" updates="None" val="True" valType="bool"/>
        <Param name="units" updates="None" val="from exp settings" valType="str"/>
      </PolygonComponent>
      <PolygonComponent name="bR">
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="durationEstim" updates="None" val="" valType="code"/>
        <Param name="fillColor" updates="set every repeat" val="$[1,1,1]" valType="str"/>
        <Param name="fillColorSpace" updates="constant" val="rgb" valType="str"/>
        <Param name="interpolate" updates="constant" val="linear" valType="str"/>
        <Param name="lineColor" updates="constant" val="$[1,1,1]" valType="str"/>
        <Param name="lineColorSpace" updates="constant" val="rgb" valType="str"/>
        <Param name="lineWidth" updates="constant" val="1" valType="code"/>
        <Param name="nVertices" updates="constant" val="4" valType="int"/>
        <Param name="name" updates="None" val="bR" valType="code"/>
        <Param name="opacity" updates="constant" val="1" valType="code"/>
        <Param name="ori" updates="constant" val="0" valType="code"/>
        <Param name="pos" updates="constant" val="(0.8, 0)" valType="code"/>
        <Param name="saveStartStop" updates="None" val="True" valType="bool"/>
        <Param name="shape" updates="constant" val="rectangle" valType="str"/>
        <Param name="size" updates="constant" val="(0.1, 0.5)" valType="code"/>
        <Param name="startEstim" updates="None" val="" valType="code"/>
        <Param name="startType" updates="None" val="time (s)" valType="str"/>
        <Param name="startVal" updates="None" val="0.0" valType="code"/>
        <Param name="stopType" updates="None" val="condition" valType="str"/>
        <Param name="stopVal" updates="constant" val="" valType="code"/>
        <Param name="syncScreenRefresh" updates="None" val="True" valType="bool"/>
        <Param name="units" updates="None" val="from exp settings" valType="str"/>
      </PolygonComponent>
      <PolygonComponent name="dot">
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="durationEstim" updates="None" val="" valType="code"/>
        <Param name="fillColor" updates="constant" val="$[1,1,1]" valType="str"/>
        <Param name="fillColorSpace" updates="constant" val="rgb" valType="str"/>
        <Param name="interpolate" updates="constant" val="linear" valType="str"/>
        <Param name="lineColor" updates="constant" val="$[1,1,1]" valType="str"/>
        <Param name="lineColorSpace" updates="constant" val="rgb" valType="str"/>
        <Param name="lineWidth" updates="constant" val="1" valType="code"/>
        <Param name="nVertices" updates="constant" val="4" valType="int"/>
        <Param name="name" updates="None" val="dot" valType="code"/>
        <Param name="opacity" updates="constant" val="1" valType="code"/>
        <Param name="ori" updates="constant" val="0" valType="code"/>
        <Param name="pos" updates="set every repeat" val="(0, 0)" valType="code"/>
        <Param name="saveStartStop" updates="None" val="True" valType="bool"/>
        <Param name="shape" updates="constant" val="rectangle" valType="str"/>
        <Param name="size" updates="constant" val="(0.1, 0.1)" valType="code"/>
        <Param name="startEstim" updates="None" val="" valType="code"/>
        <Param name="startType" updates="None" val="time (s)" valType="str"/>
        <Param name="startVal" updates="None" val="0.0" valType="code"/>
        <Param name="stopType" updates="None" val="condition" valType="str"/>
        <Param name="stopVal" updates="constant" val="" valType="code"/>
        <Param name="syncScreenRefresh" updates="None" val="True" valType="bool"/>
        <Param name="units" updates="None" val="from exp settings" valType="str"/>
      </PolygonComponent>
      <KeyboardComponent name="mykey">
        <Param name="allowedKeys" updates="constant" val="" valType="code"/>
        <Param name="correctAns" updates="constant" val="" valType="str"/>
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="discard previous" updates="constant" val="False" valType="bool"/>
        <Param name="durationEstim" updates="None" val="" valType="code"/>
        <Param name="forceEndRoutine" updates="constant" val="False" valType="bool"/>
        <Param name="name" updates="None" val="mykey" valType="code"/>
        <Param name="saveStartStop" updates="None" val="True" valType="bool"/>
        <Param name="startEstim" updates="None" val="" valType="code"/>
        <Param name="startType" updates="None" val="time (s)" valType="str"/>
        <Param name="startVal" updates="None" val="0.0" valType="code"/>
        <Param name="stopType" updates="None" val="duration (s)" valType="str"/>
        <Param name="stopVal" updates="constant" val="" valType="code"/>
        <Param name="store" updates="constant" val="all keys" valType="str"/>
        <Param name="storeCorrect" updates="constant" val="False" valType="bool"/>
        <Param name="syncScreenRefresh" updates="constant" val="True" valType="bool"/>
      </KeyboardComponent>
      <CodeComponent name="code">
        <Param name="Begin Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="speed0 = 0.0001;&amp;#10;speedn = 32;&amp;#10;currentkey = 0;&amp;#10;previouskey = 1;&amp;#10;nkey = 0;&amp;#10;currentx = 0;&amp;#10;lastkeys = [];&amp;#10;xright = 0.7;&amp;#10;xleft = (- 0.7);&amp;#10;bL.setFillColor([0, 1, 1], {&quot;log&quot;: null});&amp;#10;bR.setFillColor([0, 1, 1], {&quot;log&quot;: null});&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="speed0 = 0.0001&amp;#10;speedn = 32&amp;#10;currentkey = 0&amp;#10;previouskey = 1&amp;#10;nkey = 0&amp;#10;currentx = 0&amp;#10;lastkeys = []&amp;#10;xright = 0.7&amp;#10;xleft = -0.7&amp;#10;bL.setFillColor([0,1,1], log=None)&amp;#10;bR.setFillColor([0,1,1], log=None)&amp;#10;" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Auto-&gt;JS" valType="str"/>
        <Param name="Each Frame" updates="constant" val="if mykey.keys != lastkeys:&amp;#10;   lastkeys = mykey.keys&amp;#10;   nkey = nkey + 1&amp;#10;   if lastkeys[nkey-1] == 'left':&amp;#10;       currentkey = -1&amp;#10;   if lastkeys[nkey-1] == 'right':&amp;#10;       currentkey = 1&amp;#10;   if currentkey != previouskey:&amp;#10;       speedn = currentkey&amp;#10;   if currentkey == previouskey:&amp;#10;       speedn = speedn * 2&amp;#10;   previouskey = currentkey&amp;#10;currentx = currentx + speedn*speed0;&amp;#10;dot.setPos((currentx, 0), log=False)&amp;#10;if ((currentx &gt; xright)):&amp;#10;    continueRoutine = False&amp;#10;    bR.setFillColor([1,0,0], log=None)&amp;#10;if ((currentx &lt; xleft)):&amp;#10;    continueRoutine = False&amp;#10;    bL.setFillColor([1,0,0], log=None)&amp;#10;" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="if ((mykey.keys !== lastkeys)) {&amp;#10;    lastkeys = mykey.keys;&amp;#10;    nkey = (nkey + 1);&amp;#10;    if ((lastkeys[(nkey - 1)] === &quot;left&quot;)) {&amp;#10;        currentkey = (- 1);&amp;#10;    }&amp;#10;    if ((lastkeys[(nkey - 1)] === &quot;right&quot;)) {&amp;#10;        currentkey = 1;&amp;#10;    }&amp;#10;    if ((currentkey !== previouskey)) {&amp;#10;        speedn = currentkey;&amp;#10;    }&amp;#10;    if ((currentkey === previouskey)) {&amp;#10;        speedn = (speedn * 2);&amp;#10;    }&amp;#10;    previouskey = currentkey;&amp;#10;}&amp;#10;currentx = (currentx + (speedn * speed0));&amp;#10;dot.setPos([currentx, 0], {&quot;log&quot;: false});&amp;#10;if ((currentx &gt; xright)) {&amp;#10;    continueRoutine = false;&amp;#10;    bR.setFillColor([1, 0, 0], {&quot;log&quot;: null});&amp;#10;}&amp;#10;if ((currentx &lt; xleft)) {&amp;#10;    continueRoutine = false;&amp;#10;    bL.setFillColor([1, 0, 0], {&quot;log&quot;: null});&amp;#10;}&amp;#10;" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="dot.draw();&amp;#10;bL.draw();&amp;#10;bR.draw();&amp;#10;win.flip();&amp;#10;core.wait(1.0);&amp;#10;" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="dot.draw()&amp;#10;bL.draw()&amp;#10;bR.draw()&amp;#10;win.flip()&amp;#10;core.wait(1.0);&amp;#10;" valType="extendedCode"/>
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="name" updates="None" val="code" valType="code"/>
      </CodeComponent>
    </Routine>
  </Routines>
  <Flow>
    <LoopInitiator loopType="TrialHandler" name="trials">
      <Param name="Selected rows" updates="None" val="" valType="str"/>
      <Param name="conditions" updates="None" val="None" valType="str"/>
      <Param name="conditionsFile" updates="None" val="" valType="str"/>
      <Param name="endPoints" updates="None" val="[0, 1]" valType="num"/>
      <Param name="isTrials" updates="None" val="True" valType="bool"/>
      <Param name="loopType" updates="None" val="random" valType="str"/>
      <Param name="nReps" updates="None" val="5" valType="code"/>
      <Param name="name" updates="None" val="trials" valType="code"/>
      <Param name="random seed" updates="None" val="" valType="code"/>
    </LoopInitiator>
    <Routine name="trial"/>
    <LoopTerminator name="trials"/>
  </Flow>
</PsychoPy2experiment>
